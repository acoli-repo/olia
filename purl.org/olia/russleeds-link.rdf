<?xml version="1.0"?>
<rdf:RDF xmlns:russleeds="http://purl.org/olia/russleeds.owl#"
  xmlns:olia="http://purl.org/olia/olia.owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns="http://purl.org/olia/russleeds-link.rdf#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://purl.org/olia/russleeds-link.rdf">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://purl.org/olia/olia.owl"/>
    <owl:imports rdf:resource="http://purl.org/olia/russleeds.owl"/>
    <owl:versionInfo>
      2007 created&lt;br/&gt; 
      2010-04-06 updated&lt;br/&gt; 
      Christian Chiarcos, chiarcos@uni-potsdam.de
    </owl:versionInfo>
    <rdfs:comment>
      Provisional linking between the Russian TreeTagger module
      (developed by Serge Sharoff at the University of Leeds) 
      and the OLiA Reference Model 
    </rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Conjunction">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Conjunction"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Verb">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Verb"/>
  </owl:Class>
  <owl:Class
    rdf:about="http://purl.org/olia/russleeds.owl#AdverbialPronoun">
    <owl:versionInfo>The linking decision
      (olia:adverb or olia:pronoun) needs clarification.</owl:versionInfo>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/olia/olia.owl#Adverb"/>
          <owl:Class rdf:about="http://purl.org/olia/olia.owl#Pronoun"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Initials">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Residual"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Interjection">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Interjection"
    />
  </owl:Class>
  <owl:Class
    rdf:about="http://purl.org/olia/russleeds.owl#PredicativePronoun">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Pronoun"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#OrdinalNumber">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#OrdinalNumber"
    />
  </owl:Class>
  <owl:Class
    rdf:about="http://purl.org/olia/russleeds.owl#PersonalPronoun">
    <rdfs:subClassOf
      rdf:resource="http://purl.org/olia/olia.owl#PersonalPronoun"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Numerative">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Numeral"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Particle">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Unique"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Adverb">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Adverb"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#SentenceEnd">
    <rdfs:subClassOf
      rdf:resource="http://purl.org/olia/olia.owl#SentenceFinalPunctuation"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Adjective">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Adjective"/>
  </owl:Class>
  <owl:Class
    rdf:about="http://purl.org/olia/russleeds.owl#AdjectivalPronoun">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Determiner"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Noun">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Noun"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Punctuation">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class
            rdf:about="http://purl.org/olia/olia.owl#LeftParentheticalPunctuation"/>
          <owl:Class
            rdf:about="http://purl.org/olia/olia.owl#RightParentheticalPunctuation"/>
          <owl:Class
            rdf:about="http://purl.org/olia/olia.owl#SentenceMedialPunctuation"
          />
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/russleeds.owl#Preposition">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Preposition"/>
  </owl:Class>
</rdf:RDF>
