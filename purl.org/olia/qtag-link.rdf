  <owl:Class
    rdf:about="http://purl.org/olia/qtag.owl#LexicalVerbPastParticiple">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/olia/olia.owl#hasTense"/>
