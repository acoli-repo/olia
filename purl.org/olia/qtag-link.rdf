    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://purl.org/olia/olia.owl#Comparative"/>
        <owl:onProperty rdf:resource="http://purl.org/olia/olia.owl#hasDegree"
        />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Adverb"/>
  </owl:Class>
