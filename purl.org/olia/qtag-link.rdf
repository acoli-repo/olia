    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class
            rdf:about="http://purl.org/olia/olia.owl#Preposition"/>
          <owl:Class rdf:about="http://purl.org/olia/olia.owl#Unique"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
</rdf:RDF>
