        <owl:onProperty rdf:resource="http://purl.org/olia/olia.owl#hasPerson"
        />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/qtag.owl#Adjective">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/olia.owl#Adjective"/>
