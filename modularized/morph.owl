  <owl:ObjectProperty rdf:about="http://purl.org/olia/olia.owl#hasSeparability">
    <!--rdfs:domain rdf:resource="http://purl.org/olia/olia.owl#Verb"/ -->
	<!-- can also be particles, cf. UbyPos -->
