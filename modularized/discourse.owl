        <rdfs:label xml:lang="en">enablement (RST)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/olia_discourse.owl#EntityBasedRelation -->

    <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#EntityBasedRelation">
