    <!-- http://purl.org/olia/discourse/olia_discourse.owl#inverseOf -->

    <owl:AnnotationProperty rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#inverseOf">
        <rdfs:comment>if two asymmetric discourse relations are equivalent in their definition and differ only in their nuclearity, they should be connected by this relation. This is used systematically to capture the dependencies between argument roles in ISO.</rdfs:comment>
        <rdfs:label xml:lang="en">inverse of</rdfs:label>
    </owl:AnnotationProperty>
    


