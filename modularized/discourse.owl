    <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#ProblemSolution_RST">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#ProblemSolution"/>
        <rdfs:label xml:lang="en">problem solution (RST)</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:Class>
    


