	<owl:Class rdf:about="http://purl.org/olia/olia.owl#Question">
      <!--dcr:datcat rdf:about="http://www.isocat.org/datcat/DC-1911"/-->
      <owl:versionInfo>Santorini 1991, Bies et al. 1995</owl:versionInfo>
      <rdfs:subClassOf>
