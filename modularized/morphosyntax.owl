						<owl:someValuesFrom rdf:resource="http://purl.org/olia/olia.owl#Countable"/>
					</owl:Restriction>
			</owl:intersectionOf>
		</owl:Class> 
    </rdfs:subClassOf>  	
  </owl:Class>
	
