	<rdfs:comment>no definition given</rdfs:comment>
	<rdfs:label xml:lang="en">onomatopoetic word</rdfs:label>
	<rdfs:label xml:lang="fr">onomatopée</rdfs:label>
	<skos:altLabel xml:lang="fr">onomatopée</skos:altLabel>
