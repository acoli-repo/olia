    >pos</olia_system:hasTier>
    <olia_system:hasTag rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >NNT1c</olia_system:hasTag>
    <hasNumber rdf:resource="#singular"/>
  </TemporalNoun>
  <ProperNoun rdf:ID="NP">
    <olia_system:hasTier rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
