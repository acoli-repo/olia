        <rdfs:comment>Person=first</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/emille.owl#ForeignWord -->

    <owl:Class rdf:about="http://purl.org/olia/emille.owl#ForeignWord">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/emille.owl#Residual"/>
    </owl:Class>
    


