    >IO</olia_system:hasTag>
  </IndirectObject>
  <MainClause rdf:ID="MAIN">
    <olia_system:hasTagContaining rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >-MAIN</olia_system:hasTagContaining>
    <olia_system:hasTag rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >MAIN</olia_system:hasTag>
  </MainClause>
  <Clause rdf:ID="S">
    <olia_system:hasTag rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >S</olia_system:hasTag>
    <startsWith rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >S</startsWith>
  </Clause>
  <NounPhrase rdf:ID="NP">
    <startsWith rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >NP</startsWith>
    <olia_system:hasTag rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >NP</olia_system:hasTag>
