  <owl:Class rdf:ID="CONST">
	<rdfs:subClassOf>
		<owl:Class rdf:about="http://purl.org/olia/system.owl#UnitOfAnnotation"/>
	</rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/olia/system.owl#hasTier"/>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >cs</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasFunction"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
