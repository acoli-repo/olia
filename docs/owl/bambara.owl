        <rdfs:comment xml:lang="fr"
            >&#224; pronom personnel non-emphatique 3&#232;me pers. sg.</rdfs:comment>
        <rdfs:comment xml:lang="fr"
            >&#237;;
            pronom personnel non-emphatique 2&#232;me pers. sg.</rdfs:comment>
    </PronomPersonnel>
    


    <!-- http://purl.org/olia/bambara.owl#ABSTR -->

    <Derivation rdf:about="#ABSTR">
        <rdf:type rdf:resource="#derive.n"/>
        <rdf:type rdf:resource="#initial.adj"/>
        <rdf:type rdf:resource="#initial.n"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <system:hasTagContaining>ABSTR</system:hasTagContaining>
        <rdfs:comment xml:lang="fr"
            >-ya;
            n, adj, (v);
            n, (v);
            nom de statut ou d&#8217;&#233;tat (d&#233;riv&#233; des noms de personnes ou de certains animaux), du nom de qualit&#233; (d&#233;riv&#233; des adjectifs) ; rarement, des verbes &#224; valeurs inchoatives</rdfs:comment>
    </Derivation>
    


    <!-- http://purl.org/olia/bambara.owl#ADJ -->

    <Derivation rdf:about="#ADJ">
        <rdf:type rdf:resource="#derive.adj"/>
        <rdf:type rdf:resource="#initial.vq"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <system:hasTagContaining>ADJ</system:hasTagContaining>
        <rdfs:comment xml:lang="fr"
            >-man;
            vq;
            adj;
            adjectiviseur des verbes qualitatifs</rdfs:comment>
    </Derivation>
    


    <!-- http://purl.org/olia/bambara.owl#AG.EX -->

    <initial.v rdf:about="#AG.EX">
        <rdf:type rdf:resource="#Derivation"/>
        <rdf:type rdf:resource="#derive.n"/>
        <rdf:type rdf:resource="#initial.adj"/>
        <rdf:type rdf:resource="#initial.n"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <rdfs:comment xml:lang="fr"
            >-nci; 
            n, adj, v;
            n;
            nom d&#8217;agent excessif</rdfs:comment>
        <system:hasTagContaining>AG.EX</system:hasTagContaining>
    </initial.v>
    


    <!-- http://purl.org/olia/bambara.owl#AG.OCC -->

    <owl:Thing rdf:about="#AG.OCC">
        <rdf:type rdf:resource="#Derivation"/>
        <rdf:type rdf:resource="#derive.n"/>
        <rdf:type rdf:resource="#initial.v"/>
        <system:hasTagContaining>AG.OCC</system:hasTagContaining>
        <rdfs:comment xml:lang="fr"
            >-baa / -baga;
            v;
            n, (adj);
            nom d&#8217;agent occasionnel</rdfs:comment>
    </owl:Thing>
    


    <!-- http://purl.org/olia/bambara.owl#AG.PRM -->

    <initial.v rdf:about="#AG.PRM">
        <rdf:type rdf:resource="#Derivation"/>
        <rdf:type rdf:resource="#derive.n"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <system:hasTagContaining>AG.PRM</system:hasTagContaining>
        <rdfs:comment xml:lang="fr"
            >-la / -na; 
            v;
            n;
