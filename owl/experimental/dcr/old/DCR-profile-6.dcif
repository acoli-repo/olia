   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">The omission of a syllable or vowel at the beginning or end of a
      word, esp. when a word ending with a vowel is next to one beginning with a vowel. </dcif:definition>
     <dcif:source>www.wordreference.com/English/definition.asp?en=elision </dcif:source>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> cats 'n dogs = 'cats and dogs' </dcif:example>
     <dcif:source> www.wikipedia.org/wiki/Elative_case </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">elision</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">L'omission d'une syllabe ou d'une voyelle au début ou à la fin
      d'un mot, spécialement quand le mot finissant par la voyelle est à côté d'un mot commençant
      par une voyelle.</dcif:definition>
     <dcif:source>Gil Francopoulo</dcif:source>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">élision</dcif:name>
