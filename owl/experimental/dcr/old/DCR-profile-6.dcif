 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1231" type="simple">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
    <dcif:identifier>adposition</dcif:identifier>
    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:origin>?</dcif:origin>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Part of speech that occurs before/inside/after a complement
      composed of a noun phrase, noun, pronoun or clause that functions as a noun phrase and form a
      single structure with the complement to express its grammatical and semantic relation to
      another unit.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="en">Is a cover term for preposition, circumposition and
      postposition.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">adposition</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Partie du discours qui apparaît avant/dans/après un complément
      composé d'un groupe nominal, nom, pronom ou clause qui fonctionne comme un groupe nominal et
      forme une structure avec le complément pour exprimer sa relation grammaticale et sémantique
      avec une autre unité.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">adposition</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
 </dcif:dataCategory>
 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1232" type="simple">
