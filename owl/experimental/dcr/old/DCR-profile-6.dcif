 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1425" type="complex">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
    <dcif:identifier>morphologicalFeature</dcif:identifier>
    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en"/>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Property attached to a given inflected form that usually permits
      to distinguish this form from the generic lemmatised form of the word.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="en">For instance, the inflected form "tables" has the morphological
      feature /grammatical number/ that is valued with /plural/ and so, permits to distinguish
      "tables" from "table".</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">morphological feature</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Propriété associée à une forme fléchie qui habituellement permet
      de distinguer cette forme de la forme générique lemmatisée du mot.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">trait morphologique</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
  <dcif:conceptualDomain type="open">
   <dcif:dataType>string</dcif:dataType>
  </dcif:conceptualDomain>
 </dcif:dataCategory>
 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-2275" type="complex">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
    <dcif:identifier>namedEntity</dcif:identifier>
    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:creation>
     <dcif:creationDate>2007-10-31</dcif:creationDate>
     <dcif:changeDescription xml:lang="en"/>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">segment of text for which one or many rigid designators stands
      for the referent</dcif:definition>
     <dcif:source>Gil Francopoulo</dcif:source>
     <dcif:note xml:lang="en">usually named entities are located and classified into predefined
      types such as names of person, organizations, locations, expressions of times etc.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">named entity</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">segment de texte pour lequel un ou plusieurs indices rigides
      permettent de désigner le référent</dcif:definition>
     <dcif:source>Gil Francopoulo</dcif:source>
     <dcif:note xml:lang="fr">habituellement, les entités nommés sont localisées et classifiées en
      types comme noms de personne, organisations, toponymes, expressions temporelles
      etc.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">entité nommée</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
  <dcif:conceptualDomain type="open">
   <dcif:dataType>string</dcif:dataType>
  </dcif:conceptualDomain>
 </dcif:dataCategory>
