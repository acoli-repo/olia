 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1258" type="simple">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
    <dcif:identifier>conditional</dcif:identifier>
    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:origin>?</dcif:origin>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">A conditional relation is a logical relation in which the
      illocutionary act employing one of a pair of propositions is expressed or implied to be true
      or in force if the other proposition is true. </dcif:definition>
     <dcif:source>www.sil.org/linguistics/GlossaryOfLinguisticTerms/WhatIsAConditionalRelation.htm
     </dcif:source>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> ``If he comes' is a conditional clause in the sentence ``If he
      comes I shall go' </dcif:example>
     <dcif:source> www.wordreference.com/English/definition.asp?en=conditional </dcif:source>
    </dcif:exampleSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> If you give her the ring, then you are married to her.I hereby
      pronounce you married.Are you married to her?Do marry her.If only you would marry her! </dcif:example>
     <dcif:source> www.atilf.inalf.fr Tlfi, COMPLEMENT, C.1, Gramm. </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">conditional</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Une relation conditionnelle est une relation logique dans
      laquelle l'acte illocutoire employant une des paires de propositions est exprimé ou
      sous-entendu comme étant vrai ou déduit si l'autre proposition est vraie.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">conditionnel</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
 </dcif:dataCategory>
