 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1424" type="simple">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
    <dcif:identifier>verb</dcif:identifier>
    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:effectiveDate>2004-04-23</dcif:effectiveDate>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en"/>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:profile>Terminology</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Element which, singly or in combination with other verbs is used
      as the minimal predicate of a sentence, co-occurring with a subject. If the predicate contains
      other elements (e.g. object, complement), then it is the verb which more than any other is the
      unit which influences the choice and extent of these elements.</dcif:definition>
     <dcif:source>Crystal 2003</dcif:source>
     <dcif:note xml:lang="en">A verb (in a lot of languages) expresses morphological features like
      tense, mood or person.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">verb</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Element qui tout seul ou en combinaison avec d'autres verbes est
      utilisé comme le prédicat minimal d'une phrase, co-présent avec un sujet. Si le prédicat
      contient d'autres éléments (ex: objet, complément), alors le verbe est le mot qui plus que
      tout autre influence le choix et l'extension de ces éléments.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">verbe</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
 </dcif:dataCategory>
 <dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1427" type="complex">
  <dcif:administrationInformationSection>
   <dcif:administrationRecord>
