    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:origin>?</dcif:origin>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Change in the form of a linguistic unit, usually modification in
      the base/root or affixation to create a new word.</dcif:definition>
     <dcif:source>Sue Ellen Wright + Gil Francopoulo</dcif:source>
     <dcif:note xml:lang="en">Sometimes derivation signals a change in part of speech, such as
      "nation" to "nationalize". Sometimes the part of speech remains the same as in
      "nationalization" vs "denationalization".</dcif:note>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> Kindness is derived from kind </dcif:example>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">derivation</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Changement dans la forme d'un mot qui a pour effet de créer un
      nouveau mot en modifiant sa base ou une de ses affixations.</dcif:definition>
     <dcif:source>Gil Francopoulo</dcif:source>
     <dcif:note xml:lang="fr">Quelque fois la dérivation signale un changement de partie du discours
      (ex: "nation" qui donne "nationaliser") mais dans d'autres circonstances, la partie du
      discours reste la même (ex: "nationalisation" et "dénationalisation").</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">dérivation</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
  <dcif:conceptualDomain type="open">
   <dcif:dataType>string</dcif:dataType>
  </dcif:conceptualDomain>
