  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Part of speech to refer to an heterogeneous group of words whose
      most frequent function is to specify the mode of action of the verb.</dcif:definition>
     <dcif:source>Crystal 2003</dcif:source>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> She threw the ball far, She threw the ball very far, An extremely
      big horse </dcif:example>
     <dcif:source> www.sil.org/linguistics/GlossaryOfLinguisticTerms/WhatIsAnAdposition.htm
     </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">adverb</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Partie du discours qui réfère à un groupe hétérogène de mots
      dont l'action la plus fréquente est de spécifier le mode d'action du verbe.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">adverbe</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
 </dcif:dataCategory>
