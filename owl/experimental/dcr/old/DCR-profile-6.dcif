    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:origin>?</dcif:origin>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">One of two or more complementary morphs which manifest a
      morpheme in its different phonological or morphological environments. </dcif:definition>
     <dcif:source> www.sil.org/linguistics/GlossaryOfLinguisticTerms/WhatIsAnAllomorph.htm </dcif:source>
     <dcif:note xml:lang="en">"s" in "cots" and "digs" has the allomorphs represented phonetically
      by {-s} and {-z} respectively.</dcif:note>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> [-s] as in [hQts] 'hats' </dcif:example>
     <dcif:source> www.wikipedia.org/wiki/Allative_case </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">allomorph</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Un ou deux morphes complémentaires qui manifestent un morphème
      dans ses différentes situations phonétiques ou morphologiques.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="fr">En anglais, "s" dans "cots" et "digs" manifeste une allomorphie
      vis-à-vis de {-s} et {-z} respectivement.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">allomorphe</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
