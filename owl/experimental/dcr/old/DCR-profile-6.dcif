   <dcif:profile>Terminology</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Register of a term or text type that can be characterized as
      profane or socially unacceptable.</dcif:definition>
     <dcif:source>ISO12620</dcif:source>
    </dcif:definitionSection>
    <dcif:explanationSection>
     <dcif:explanation xml:lang="en">Note: Although vulgar register is avoided in formal technical
      terminology, languages with broad distribution such as English or Spanish can require the
      documentation of problematic terms that vary in register from region to
      region.</dcif:explanation>
     <dcif:source>Mitre; TEI</dcif:source>
    </dcif:explanationSection>
