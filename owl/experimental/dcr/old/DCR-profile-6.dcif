    <dcif:version>1:0</dcif:version>
    <dcif:registrationStatus>private</dcif:registrationStatus>
    <dcif:origin>?</dcif:origin>
    <dcif:creation>
     <dcif:creationDate>2004-07-09</dcif:creationDate>
     <dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
    </dcif:creation>
   </dcif:administrationRecord>
  </dcif:administrationInformationSection>
  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Term referring to a situation where the grammatical subject is
      typically the recipient or goal of the action denoted by the verb.</dcif:definition>
     <dcif:source>Crystal 2003</dcif:source>
     <dcif:note xml:lang="en">Passive contrasts with active voice and for some languages like Greek
      with middle voice.</dcif:note>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> My lunch was eaten by them </dcif:example>
     <dcif:source> www.atilf.inalf.fr-&gt; Tlfi, 2.PASSIF C.1,a </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">passive voice</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Terme référant à une situation où le sujet grammatical is
      typiquement le récipiendaire ou le but de l'action dénotée par le verbe.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="fr">La passif est en opposition avec l'actif et pour quelques langues
      comme le grec avec le milieu.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">voix passive</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
 </dcif:dataCategory>
