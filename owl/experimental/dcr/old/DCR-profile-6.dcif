  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">process to modify the sense of a word by some operations to
      repeat the sound of a word.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="en">Reduplication may be found in many Asian languages such as Chinese,
      Japanese, Thai, Maly, or Bangla. The operation works on each syllable (or character) of the
      word.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">reduplication</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">processus de modification du sens d'un mot par répétition de la
      prononciation du mot.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="fr">La duplication apparaît dans un certain nombre de langue asiatiques
      comme le chinois, le japonais, le thai, le malais et le bengali. L'opération fonctionne sur
      chacune des syllabes (ou caractères) du mot.</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">duplication</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
