  <dcif:descriptionSection>
   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Case that expresses the partial nature of the referent of the
      noun it marks, as opposed to expressing the whole unit or class of which the referent is a
      part.</dcif:definition>
     <dcif:source>DFKI</dcif:source>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">partitive case</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Cas qui exprime la nature partielle du référent du nom qu'il
      marque en opposition à l'expression de la totalité de l'unité ou de la classe dont le référent
      est une partie.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">cas partitif</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
