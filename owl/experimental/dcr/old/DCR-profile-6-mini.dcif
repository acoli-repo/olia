    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Part of speech related to attributes of noun.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="en">We speak of an adjective when one can ask the question: how is
      something?</dcif:note>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en"> A big horse </dcif:example>
     <dcif:source> www.southwestern.edu/~carlg/Latin_Web/glossary.html </dcif:source>
    </dcif:exampleSection>
    <dcif:nameSection>
     <dcif:name xml:lang="en">adjective</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Partie du discours pour les attributs du nom.</dcif:definition>
     <dcif:source/>
     <dcif:note xml:lang="fr">On parle d'un adjectif quand on peut poser la question: comment est
      une chose ?</dcif:note>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">adjectif</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
