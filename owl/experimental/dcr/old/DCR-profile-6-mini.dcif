   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Case used to typically indicate locative or instrumental
      function. </dcif:definition>
     <dcif:source>Crystal 2003 </dcif:source>
     <dcif:note xml:lang="en">The case is usually translated into English with a
      preposition.</dcif:note>
    </dcif:definitionSection>
    <dcif:exampleSection>
     <dcif:example xml:lang="en">By the window, in the doghouse</dcif:example>
    </dcif:exampleSection>
    <dcif:note xml:lang="en"> Ablative is one of the six locative cases which as their basic meaning
     correspond to locational prepositions in English. </dcif:note>
    <dcif:nameSection>
     <dcif:name xml:lang="en">ablative case</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
