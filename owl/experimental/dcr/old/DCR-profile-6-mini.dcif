   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Manière dont les propositions peuvent changer la relation entre
      le sujet et l'objet du verbe sans changer le sens de la proposition.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">voix</dcif:name>
