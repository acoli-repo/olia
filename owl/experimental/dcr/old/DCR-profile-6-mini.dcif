   <dcif:profile>Morphosyntax</dcif:profile>
   <dcif:languageSection>
    <dcif:language>en</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="en">Case which expresses the meaning of presence 'at' or 'near' a
      place.</dcif:definition>
     <dcif:source>Crystal 2003</dcif:source>
    </dcif:definitionSection>
    <dcif:note xml:lang="en"> Adessive is one of the six locative cases which as their basic meaning
     correspond to locational prepositions in English. </dcif:note>
    <dcif:nameSection>
     <dcif:name xml:lang="en">adessive case</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
   <dcif:languageSection>
    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Cas qui exprime le sens de la présence 'à' ou 'à côté' d'un
      endroit.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">cas adessif</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
    </dcif:nameSection>
   </dcif:languageSection>
  </dcif:descriptionSection>
