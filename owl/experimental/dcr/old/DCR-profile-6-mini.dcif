    <dcif:language>fr</dcif:language>
    <dcif:definitionSection>
     <dcif:definition xml:lang="fr">Registre d'un type de texte qui peut être caractérisé comme
      étant profane ou socialement inacceptable.</dcif:definition>
     <dcif:source/>
    </dcif:definitionSection>
    <dcif:nameSection>
     <dcif:name xml:lang="fr">registre vulgaire</dcif:name>
     <dcif:nameStatus>standardized name</dcif:nameStatus>
