    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="morphemeFollowsElement"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="morphemeWordInitial"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="morphemeWordFinal"/>
    </owl:disjointWith>
    <owl:disjointWith>
