  </owl:Class>
  <owl:Class rdf:ID="neutralFeature">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="roundingFeatureNode"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >neutral feature</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="larygealNTN">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >laryngeal</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="non-terminalNode"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="quadralNumber">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
