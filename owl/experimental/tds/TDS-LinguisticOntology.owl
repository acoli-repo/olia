    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#adverbial"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#determiner"/>
    </owl:disjointWith>
  </owl:Class>
