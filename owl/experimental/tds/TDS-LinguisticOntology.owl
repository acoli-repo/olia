    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="adverbialModifier">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="modifyingRelation"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
