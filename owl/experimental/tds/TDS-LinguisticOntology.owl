    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >quadral number</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#numberProperty"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#trialNumber"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#generalNumber"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="dualNumber"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#singularNumber"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#collectiveNumber"/>
  </owl:Class>
  <owl:Class rdf:ID="placeHead">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >place head</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="dependencyPhonologyHierarchyElement"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="mannerHead"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="mannerDependent"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="DP-root"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="placeDependent"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="mannerSubhead"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="bound">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A sublexical morphological unit is bound if it may not by itself constitute a word.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >bound</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="boundFree"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="honorificProperty">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Distinguishing levels of politeness or respect especially in relation to 
    compared social status (&lt;reference id="8" type="bib"&gt;Crystal 1985&lt;/reference&gt;).</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#semanticProperty"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >honorific</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="inverseMarkerFunction">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >inverse marker function</rdfs:label>
    <rdfs:subClassOf rdf:resource="#direct-inverseMarkerFunction"/>
  </owl:Class>
  <owl:Class rdf:ID="consonantDisharmony">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Word-level rules stating obligatory disagreement of consonants with respect to one or more features.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="disharmony"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >consonant disharmony</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="palatoAlveolarNasal">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >palato-alveolar nasal</rdfs:label>
    <rdfs:subClassOf rdf:resource="#nasal"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A nasal for which the target of the active articulator (in this case 
    the tongue blade) is the region between the alveolar ridge and the palate.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#intonationUnits">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Changes in fundamental frequency during the course of an utterance. 
    Intonation carries information that is not provided by the stream of 
    consonants and vowels. It might tell the listener whether the sentence is 
    a question or a statement, or whether more will follow. Intonation may 
    also signal differences in meaning or in attitude &lt;reference id="40" type="bib"&gt;(Ackema, 2001)&lt;/reference&gt;.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TODO: Ceclia Odé</owl:versionInfo>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="prosodicElement"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >intonation units</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="consonantClusterSize">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="syllableStructureProperty"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The number of possible consonants in an onset or coda cluster. This may depend on the syllable's positon in a word.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >consonant cluster size</rdfs:label>
    <label-alias rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >consonant cluster length</label-alias>
  </owl:Class>
  <owl:Class rdf:about="#dualNumber">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >dual number</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#generalNumber"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#collectiveNumber"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#trialNumber"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#singularNumber"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Two individual entities or three groups of entities.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#numberProperty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="terminalNode">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="featureGeometryHierarchyNode"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >terminal node</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#NounClass_01">
    <owl:disjointWith rdf:resource="#NounClass_06"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#NounClass_08"/>
    </owl:disjointWith>
    <owl:disjointWith>
