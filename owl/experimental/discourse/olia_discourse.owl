  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#TextualOrganizationalRelation">
    <rdfs:comment>cf. SemDok TextualRelation</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#TextualOrganizationalRelation">
    <rdfs:comment>"Pseudo-relations" that represent conventional text-structural relationships between text spans, not strictly speaking discourse relations.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#TextualOrganizationalRelation">
    <rdfs:label>textual organizational relation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#TextualOrganizationalRelation">
    <rdfs:label xml:lang="en">textual organizational relation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#TextualOrganizationalRelation">
    <rdfs:label>textual relation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Time">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Time">
