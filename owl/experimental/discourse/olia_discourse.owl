  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#FactualCondition_HDRB">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#FactualCondition_HDRB">
