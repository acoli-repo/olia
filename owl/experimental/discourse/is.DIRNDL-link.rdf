<?xml version="1.0"?>
<rdf:RDF
    xmlns:stuttgart="http://purl.org/olia/discourse/is.DIRNDL.owl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:is="http://purl.org/olia/discourse/olia_discourse.owl#"
    xmlns="http://purl.org/olia/discourse/is.DIRNDL-link.rdf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://purl.org/olia/discourse/is.DIRNDL-link.rdf">
  <owl:Ontology rdf:about="http://purl.org/olia/discourse/is.DIRNDL-link.rdf">
    <owl:versionInfo>
		2008-05-28 created
		2012-02-10 submitted to SourceForge
		2014-03-22 updated
						
		Christian Chiarcos, christian.chiarcos@web.de
	</owl:versionInfo>
    <owl:imports>
	<owl:Ontology rdf:about="http://purl.org/olia/discourse/olia_discourse.owl"/>
	</owl:imports>
    <owl:imports>
	<owl:Ontology rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl"/>
	</owl:imports>
  </owl:Ontology>
  <is:NonReferring rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#expletive"/>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Definit">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#Definite"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#D-Given">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#Given"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Referierend">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#Referring"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Informationsstatus">
        <rdfs:subClassOf>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Referentiality"/>
              <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#InformationStatus"/>
              <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Definiteness"/>
              <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#BridgingType"/>
            </owl:unionOf>
          </owl:Class>
        </rdfs:subClassOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Markable">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#Markable"/>
  </owl:Class>
  <is:NonReferring rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#negative"/>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Indefinit">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Indefinite"/>
          <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#New"/>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <is:Active rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#dGivenPronoun">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Active elements are more likely to be pronominalized, therefore dGivenPronoun is certainly Active in the sense of the reference model.</rdfs:comment>
  </is:Active>
  <is:ElementOf rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#partitivePart">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This &lt;b&gt;seems&lt;/b&gt; to be ElementOf only.</rdfs:comment>
  </is:ElementOf>
  <is:SameType rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#bridgingSame"/>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#ReferierenderAusdruck">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#ReferringExpression"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Bridging">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#BridgingType"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#Inferrable"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#Partitiv">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#BridgingType"/>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Indefinite"/>
          <owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Accessible"/>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <is:PartWhole rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#bridgingPart">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This &lt;b&gt;seems&lt;/b&gt; to include set and subset relations, too, cf. Partitive-part.</rdfs:comment>
  </is:PartWhole>
  <is:SameType rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#partitiveSame"/>
  <owl:Class rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#AccessibleViaDescription">
    <rdfs:subClassOf rdf:resource="http://purl.org/olia/discourse/olia_discourse.owl#New"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hearer-new, thus New</rdfs:comment>
  </owl:Class>
  <is:WholePart rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#bridgingSuper"/>
  <is:Given rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#dGiven"/>
  <is:GenerallyAccessible rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#accessibleGeneralToken"/>
  <is:WholePart rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#partitiveSuper"/>
  <is:New rdf:about="http://purl.org/olia/discourse/is.DIRNDL.owl#accessibleGeneralType">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >These are so-called "weak definites", and I hesitate to postulate that the corresponding (generic) referent can always be inferred from the preceding context. Therefore, I suggest that these are also New, but Definite.</rdfs:comment>
  </is:New>

	<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Definite"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#GenerallyAccessible"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#ReferringExpression"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#PartWhole"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#SameType"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Referring"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#NonReferring"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Active"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#ElementOf"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#WholePart"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Inferrable"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Given"/>
<owl:Class rdf:about="http://purl.org/olia/discourse/olia_discourse.owl#Markable"/>

</rdf:RDF>

