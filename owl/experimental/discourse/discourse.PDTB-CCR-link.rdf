        <rdfs:subClassOf rdf:resource="&ccr;Causal"/>
        <rdfs:subClassOf rdf:resource="&ccr;Negative"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;Antichronological"/>
