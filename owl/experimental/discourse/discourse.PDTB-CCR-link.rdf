    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ccr "http://purl.org/olia/discourse/discourse.CCR.owl#" >
    <!ENTITY discourse "http://purl.org/olia/discourse/discourse.PDTB.owl#" >
