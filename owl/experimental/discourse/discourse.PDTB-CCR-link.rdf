    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Additive -->

    <owl:Class rdf:about="&ccr;Additive"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Antichronological -->

    <owl:Class rdf:about="&ccr;Antichronological">
        <rdfs:subClassOf rdf:resource="#Temporal"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Causal -->

    <owl:Class rdf:about="&ccr;Causal"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#CauseConsequence -->

    <owl:Class rdf:about="&ccr;CauseConsequence"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Chronological -->

    <owl:Class rdf:about="&ccr;Chronological">
        <rdfs:subClassOf rdf:resource="#Temporal"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Conditional -->

    <owl:Class rdf:about="&ccr;Conditional"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#ConsequenceCause -->

    <owl:Class rdf:about="&ccr;ConsequenceCause"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Negative -->

    <owl:Class rdf:about="&ccr;Negative"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#NonConditionalCause -->

    <owl:Class rdf:about="&ccr;NonConditionalCause"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Objective -->

    <owl:Class rdf:about="&ccr;Objective"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Positive -->

    <owl:Class rdf:about="&ccr;Positive"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Subjective -->

    <owl:Class rdf:about="&ccr;Subjective"/>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Synchronuous -->

    <owl:Class rdf:about="&ccr;Synchronuous">
        <rdfs:subClassOf rdf:resource="#Temporal"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.CCR.owl#Temporality -->

    <owl:Class rdf:about="&ccr;Temporality"/>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Asynchronous -->

    <owl:Class rdf:about="&discourse;Asynchronous">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <rdf:Description rdf:about="&ccr;Objective"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf rdf:resource="&ccr;Synchronuous"/>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#COMPARISON -->

    <owl:Class rdf:about="&discourse;COMPARISON">
        <rdfs:subClassOf rdf:resource="&ccr;Negative"/>
        <rdfs:comment
            >Sanders et al. 2018, p.60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#CONTIGENCY -->

    <owl:Class rdf:about="&discourse;CONTIGENCY">
        <rdfs:subClassOf rdf:resource="&ccr;Causal"/>
        <rdfs:subClassOf rdf:resource="#Temporal"/>
        <rdfs:comment
            >Sanders et al. 2018, p.21

&quot;In the other frameworks, relations that are causal are not considered
to be TEMPORAL relations, but their segments can be ordered in time, and
hence can also be assigned the value &#8220;temporal&#8221; along CCR&#8217;s dimension.&quot; (p.29)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Cause -->

    <owl:Class rdf:about="&discourse;Cause">
        <rdfs:subClassOf rdf:resource="&ccr;NonConditionalCause"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Causal"/>
                    <rdf:Description rdf:about="&ccr;Objective"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.23,24,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Concession -->

    <owl:Class rdf:about="&discourse;Concession">
        <rdfs:subClassOf rdf:resource="&ccr;Causal"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Objective"/>
                    <rdf:Description rdf:about="&ccr;Subjective"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.21,24,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Condition -->

    <owl:Class rdf:about="&discourse;Condition">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Conditional"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Objective"/>
                    <rdf:Description rdf:about="&ccr;Subjective"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;Antichronological"/>
                            <rdf:Description rdf:about="&ccr;ConsequenceCause"/>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;CauseConsequence"/>
                            <rdf:Description rdf:about="&ccr;Chronological"/>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.23,24,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Conjunction -->

    <owl:Class rdf:about="&discourse;Conjunction">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&ccr;Causal"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;Objective"/>
                            <rdf:Description rdf:about="&ccr;Subjective"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.20,21,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Contrast -->

    <owl:Class rdf:about="&discourse;Contrast">
        <rdfs:subClassOf rdf:resource="&ccr;Additive"/>
        <rdfs:subClassOf rdf:resource="&ccr;Objective"/>
        <rdfs:comment
            >Sanders et al. 2018, p.22,24,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#EXPANSION -->

    <owl:Class rdf:about="&discourse;EXPANSION">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <rdf:Description rdf:about="&ccr;Causal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >&quot;all relations in the PDTB classes EXPANSION are additive &#8211;
apart from DISJUNCTIVE, which can also be causal-conditional&quot;

Sanders et al. 2018, p.21</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Exception -->

    <owl:Class rdf:about="&discourse;Exception">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
