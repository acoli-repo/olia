            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Negative"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;Conditional"/>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&ccr;Additive"/>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="&ccr;Causal"/>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <owl:Class>
                                                        <owl:intersectionOf rdf:parseType="Collection">
                                                            <rdf:Description rdf:about="&ccr;Antichronological"/>
                                                            <rdf:Description rdf:about="&ccr;ConsequenceCause"/>
                                                        </owl:intersectionOf>
                                                    </owl:Class>
                                                    <owl:Class>
                                                        <owl:intersectionOf rdf:parseType="Collection">
                                                            <rdf:Description rdf:about="&ccr;CauseConsequence"/>
                                                            <rdf:Description rdf:about="&ccr;Chronological"/>
                                                        </owl:intersectionOf>
                                                    </owl:Class>
                                                </owl:unionOf>
                                            </owl:Class>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:intersectionOf>
                    </owl:Class>
