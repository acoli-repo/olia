     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://purl.org/olia/discourse/discourse.CCR.owl"/>
        <owl:imports rdf:resource="http://purl.org/olia/discourse/discourse.PDTB.owl"/>
    </owl:Ontology>
    


