<rdf:RDF xmlns="http://www.semanticweb.org/chiarcos/ontologies/2021/1/3/Ontology1612350185920.owl#"
     xml:base="http://www.semanticweb.org/chiarcos/ontologies/2021/1/3/Ontology1612350185920.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:discourse="http://purl.org/olia/discourse/discourse.PDTB.owl#"
     xmlns:Ontology1612350185920="http://www.semanticweb.org/chiarcos/ontologies/2021/1/3/Ontology1612350185920.owl#"
