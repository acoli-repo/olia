    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#PragmaticCause -->

    <owl:Class rdf:about="&discourse;PragmaticCause">
        <rdfs:subClassOf rdf:resource="&ccr;NonConditionalCause"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Causal"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                    <rdf:Description rdf:about="&ccr;Subjective"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p,24,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#PragmaticCondition -->

    <owl:Class rdf:about="&discourse;PragmaticCondition">
        <rdfs:subClassOf rdf:resource="&ccr;Conditional"/>
        <rdfs:subClassOf rdf:resource="&ccr;Subjective"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Causal"/>
                    <rdf:Description rdf:about="&ccr;Conditional"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                    <rdf:Description rdf:about="&ccr;Subjective"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;CauseConsequence"/>
                            <rdf:Description rdf:about="&ccr;ConsequenceCause"/>
