                            <rdf:Description rdf:about="&ccr;Subjective"/>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&ccr;Causal"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.21,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#Instantiation -->

    <owl:Class rdf:about="&discourse;Instantiation">
        <rdfs:subClassOf rdf:resource="&ccr;Positive"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&ccr;Causal"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Objective"/>
                    <rdf:Description rdf:about="&ccr;Subjective"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.21,60</rdfs:comment>
    </owl:Class>
    


