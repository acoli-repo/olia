                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <rdf:Description rdf:about="&ccr;Positive"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&ccr;Objective"/>
                            <rdf:Description rdf:about="&ccr;Subjective"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Additive"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&ccr;Causal"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.21,60</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#contraExpectation -->

    <owl:Class rdf:about="&discourse;contraExpectation">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ccr;Causal"/>
                    <rdf:Description rdf:about="&ccr;CauseConsequence"/>
                    <rdf:Description rdf:about="&ccr;Chronological"/>
                    <rdf:Description rdf:about="&ccr;Negative"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment
            >Sanders et al. 2018, p.17,60

features according to p.60. p.17 sloightly deviating</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/discourse/discourse.PDTB.owl#disjunctive -->

    <owl:Class rdf:about="&discourse;disjunctive">
