    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#denotates -->

    <owl:ObjectProperty rdf:about="&multext-east;denotates">
        <rdfs:comment rdf:datatype="&xsd;string">Relation between linguistic expressions and their semantic content.</rdfs:comment>
        <rdfs:range rdf:resource="&multext-east;DiscourseEntity"/>
        <rdfs:domain rdf:resource="&multext-east;Markable"/>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasBridgingType -->

    <owl:ObjectProperty rdf:about="&multext-east;hasBridgingType">
        <rdfs:range rdf:resource="&multext-east;BridgingType"/>
        <rdfs:subPropertyOf rdf:resource="&multext-east;hasPragmaticFeature"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&multext-east;BridgingRelation"/>
                    <rdf:Description rdf:about="&multext-east;ReferringExpression"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasInformationStatus -->

    <owl:ObjectProperty rdf:about="&multext-east;hasInformationStatus">
        <rdfs:comment rdf:datatype="&xsd;string">Relation between discourse referents and information status, and (mediated by the denotates relationship) between referring expressions and information status.</rdfs:comment>
        <rdfs:range rdf:resource="&multext-east;InformationStatus"/>
        <rdfs:subPropertyOf rdf:resource="&multext-east;hasPragmaticFeature"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&multext-east;DiscourseReferent"/>
                    <rdf:Description rdf:about="&multext-east;ReferringExpression"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasPragmaticFeature -->

    <owl:ObjectProperty rdf:about="&multext-east;hasPragmaticFeature">
        <rdfs:comment rdf:datatype="&xsd;string">Relation between discourse entities and pragmatic features, and (mediated by the denotates relationship) between markables and pragmatic features.</rdfs:comment>
        <rdfs:range rdf:resource="&multext-east;PragmaticFeature"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&multext-east;DiscourseEntity"/>
                    <rdf:Description rdf:about="&multext-east;Markable"/>
                    <rdf:Description rdf:about="&multext-east;PragmaticRelation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasSource -->

    <owl:ObjectProperty rdf:about="&multext-east;hasSource">
        <rdfs:domain rdf:resource="&multext-east;PragmaticRelation"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&multext-east;AnnotationUnit"/>
                    <rdf:Description rdf:about="&multext-east;PragmaticCategory"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasStructuralFocusType -->

    <owl:ObjectProperty rdf:about="&multext-east;hasStructuralFocusType">
        <rdfs:domain rdf:resource="&multext-east;Focus"/>
        <rdfs:range rdf:resource="&multext-east;StructuralFocusType"/>
        <rdfs:subPropertyOf rdf:resource="&multext-east;hasPragmaticFeature"/>
    </owl:ObjectProperty>
    


    <!-- file:///C:/Users/Christian/Desktop/onto/owl/experimental/multext-east/multext-east.owl#hasTarget -->

    <owl:ObjectProperty rdf:about="&multext-east;hasTarget">
        <rdfs:domain rdf:resource="&multext-east;PragmaticRelation"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&multext-east;AnnotationUnit"/>
                    <rdf:Description rdf:about="&multext-east;PragmaticCategory"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


