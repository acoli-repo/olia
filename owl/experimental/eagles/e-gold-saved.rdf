<?xml version="1.0" encoding="UTF-8"?>
<!-- Achtung: muss immer eigenen namespace haben, namespace auf e-eagles.owl umzulegen, scheitert 
	alternativ darf namespace evtl. nicht zu file zeigen, sondern nur http ??? -->
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.owl-ontologies.com/Ontology11485532631.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:gold="http://localhost:8080/sfb-is/en/owl/gold03.owl#"
    xmlns:e-eagles="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
  xml:base="http://www.owl-ontologies.com/Ontology11485532631.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl"/>
    <owl:imports rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >illustrates linking between extended eagles-based upper model and the GOLD 0.3 ontology</rdfs:label>
  </owl:Ontology>
<rdf:Description rdf:ID="Postposition">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Postposition">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Postposition"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#SubordinatingConnective">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="SubordinatingConjunction">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#SubordinatingConnective"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="DefiniteArticle">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#DefiniteArticle">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#DefiniteArticle"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="ProperNoun">
    <rdfs:subClassOf rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Substantive"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Particle">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="Unique">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Particle"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Numeral">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Numeral">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Numeral"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Quantifier">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="Quantifier">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Quantifier"/>
      </rdf:Description>
    </owl:equivalentClass>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TODO: should not be a sub-concept of Determiner as quantifiers do not necessarily appear as noun modifiers</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:ID="ing">
    <rdfs:subClassOf>
      <rdf:Description rdf:ID="Gerund">
        <owl:equivalentClass>
          <rdf:Description rdf:ID="VerbalNoun">
            <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Gerund"/>
            <owl:equivalentClass>
              <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Gerund">
                <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#VerbalNoun"/>
              </rdf:Description>
            </owl:equivalentClass>
          </rdf:Description>
        </owl:equivalentClass>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Participle"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Connective">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="Conjunction">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Connective"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#IndefiniteArticle">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="IndefiniteArticle">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#IndefiniteArticle"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Nominal">
    <rdfs:subClassOf rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#CommonNoun"/>
  </rdf:Description>
  <rdf:Description rdf:ID="Adverb">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Adverb">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Adverb"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Noun">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Noun">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Noun"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Adjective">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Adjective">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Adjective"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Preposition">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Preposition">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Preposition"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Adposition">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="Adposition">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Adposition"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Article">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Article">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Article"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#CoordinatingConnective">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="CoordinatingConjunction">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#CoordinatingConnective"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Determiner">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Determiner">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >TODO: ist eine eher semantische Definition von Determinerergänze "anchoring" in der Def., dann sind Possessivpronomen eingeschlossen, nur unter dieser voraussetzung sind gold:Determiner und eagles:Determiner äquivalent</owl:versionInfo>
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/e-eagles.owl#Determiner"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Interjection">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="Interjection">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#Interjection"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#CardinalNumeral">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="CardinalNumber">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#CardinalNumeral"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Demonstrative">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TODO: should be modelled as an independent concept rather than a Determiner, cf. DemonstrativePronoun</owl:versionInfo>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:ID="DemonstrativeDeterminer"/>
          <rdf:Description rdf:ID="DemonstrativePronoun"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#OrdinalNumeral">
    <owl:equivalentClass>
      <rdf:Description rdf:ID="OrdinalNumber">
        <owl:equivalentClass rdf:resource="http://localhost:8080/sfb-is/en/owl/gold03.owl#OrdinalNumeral"/>
      </rdf:Description>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:ID="Circumposition">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Postposition"/>
          <rdf:Description rdf:about="http://localhost:8080/sfb-is/en/owl/gold03.owl#Preposition"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </rdf:Description> 
</rdf:RDF>
