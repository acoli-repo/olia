:SCONJ rdfs:label "subordinating conjunction"@en;  a u:SCONJ;
  system:hasTag "SCONJ";
  system:hasTier "pos";
  dct:language "eu";
  rdfs:comment """ <p>This document is a placeholder for the language-specific documentation
for <code>SCONJ</code>.</p><p><a href="https://github.com/universaldependencies/docsv1/edit/pages-source/_eu-pos/SCONJ.md" target="#">edit SCONJ</a></p><p><a id="al-eu-pos/SYM" class="al-dest"/></p><h2><code>SYM</code>: symbol</h2> """^^rdf:XMLLiteral.

@prefix : <https://universaldependencies.org/docsv1/eu/pos/> .
