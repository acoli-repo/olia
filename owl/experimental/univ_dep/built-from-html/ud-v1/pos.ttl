  system:hasTag "INTJ";
  system:hasTier "pos";
  dct:language "fo";
  rdfs:comment """ <p>This document is a placeholder for the language-specific documentation
for <code>INTJ</code>.</p><p><a href="https://github.com/universaldependencies/docsv1/edit/pages-source/_fo-pos/INTJ.md" target="#">edit INTJ</a></p><p><a id="al-fo-pos/NOUN" class="al-dest"/></p><h2><code>NOUN</code>: noun</h2> """^^rdf:XMLLiteral.

