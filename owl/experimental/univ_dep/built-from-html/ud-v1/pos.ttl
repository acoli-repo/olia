@prefix : <https://universaldependencies.org/docsv1/bxr/pos/> .
@prefix u: <https://universaldependencies.org/docsv1/u/pos/> . 
:ADJ rdfs:label "adjective"@en;  a u:ADJ;
  system:hasTag "ADJ";
  system:hasTier "pos";
  dct:language "bxr";
  rdfs:comment """ <p>This document is a placeholder for the language-specific documentation
for <code>ADJ</code>.</p><p><a href="https://github.com/universaldependencies/docsv1/edit/pages-source/_bxr-pos/ADJ.md" target="#">edit ADJ</a></p><p><a id="al-bxr-pos/ADP" class="al-dest"/></p><h2><code>ADP</code>: adposition</h2> """^^rdf:XMLLiteral.

@prefix : <https://universaldependencies.org/docsv1/bxr/pos/> .
