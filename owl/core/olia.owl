      <rdfs:subClassOf>
         <owl:Class rdf:about="http://purl.org/olia/olia.owl#CommonNoun"/>
      </rdfs:subClassOf>
      <owl:versionInfo>subClassOf partOfSpeech (dcif:conceptualDomain)</owl:versionInfo>
   </owl:Class>  
  <owl:Class rdf:about="http://purl.org/olia/olia.owl#DiminutiveNoun">
      <!--dcr:datcat rdf:about="http://www.isocat.org/datcat/DC-2225"/-->
