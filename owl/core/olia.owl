<owl:Class rdf:about="http://purl.org/olia/olia.owl#LeftSentenceBracket">
		<rdfs:subClassOf>
			<rdf:Description rdf:about="http://purl.org/olia/olia-top.owl#TopologicalField"/>
		</rdfs:subClassOf>
		<rdfs:comment>
