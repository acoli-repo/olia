      <!--dcr:datcat rdf:about="http://www.isocat.org/datcat/DC-2222"/-->
      <owl:versionInfo>http://www.isocat.org/datcat/DC-2222</owl:versionInfo>
      <rdfs:comment>
                            conditional pronoun
                                (MIRACL &amp; LSCA; http://www.isocat.org/datcat/DC-2222)
                            </rdfs:comment>
      <rdfs:label>conditional pronoun</rdfs:label>
      <rdfs:subClassOf>
         <owl:Class rdf:about="http://purl.org/olia/olia.owl#Pronoun"/>
      </rdfs:subClassOf>
      <owl:versionInfo>subClassOf pronoun (dcif:isA)</owl:versionInfo>
	</owl:Class>
<owl:Class rdf:about="http://purl.org/olia/olia.owl#EmphaticPronoun">
      <!--dcr:datcat rdf:about="http://www.isocat.org/datcat/DC-1941"/-->
      <owl:versionInfo>http://www.isocat.org/datcat/DC-1941</owl:versionInfo>
      <rdfs:comment>
                            Pronoun marked to show its importance.
                                (http://www.isocat.org/datcat/DC-1941)
                            </rdfs:comment>
      <rdfs:label>emphatic pronoun</rdfs:label>
      <rdfs:subClassOf>
         <owl:Class rdf:about="http://purl.org/olia/olia.owl#Pronoun"/>
