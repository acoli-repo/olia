<owl:Class rdf:about="http://purl.org/olia/olia.owl#AdverbialModifier">
		<owl:versionInfo>http://languagelink.let.uu.nl/tds/onto/LinguisticOntology.owl#adverbialModifier</owl:versionInfo>
