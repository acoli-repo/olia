    <!-- http://purl.org/olia/dtm -->

    <owl:Thing rdf:about="http://purl.org/olia/dtm">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#Determinatif"/>
        <system:hasTag>dtm</system:hasTag>
        <rdfs:comment>déterminatif</rdfs:comment>
    </owl:Thing>
    


    <!-- http://purl.org/olia/pp -->

    <owl:Thing rdf:about="http://purl.org/olia/pp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#Postposition"/>
        <system:hasTag>pp</system:hasTag>
        <rdfs:comment>postposition</rdfs:comment>
    </owl:Thing>
    


    <!-- http://purl.org/olia/bambara.owl#ABSTR -->

    <owl:Thing rdf:about="http://purl.org/olia/bambara.owl#ABSTR">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#Derivation"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#derive.n"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#initial.adj"/>
        <rdf:type rdf:resource="http://purl.org/olia/bambara.owl#initial.n"/>
        <system:hasTagContaining>ABSTR</system:hasTagContaining>
        <rdfs:comment xml:lang="fr">-ya;
            n, adj, (v);
            n, (v);
            nom de statut ou d’état (dérivé des noms de personnes ou de certains animaux), du nom de qualité (dérivé des adjectifs) ; rarement, des verbes à valeurs inchoatives</rdfs:comment>
    </owl:Thing>
    


    <!-- http://purl.org/olia/bambara.owl#ADJ -->

    <owl:Thing rdf:about="http://purl.org/olia/bambara.owl#ADJ">
