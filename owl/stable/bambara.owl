    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/olia/bambara.owl#derivation -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/bambara.owl#derivation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/olia/system.owl#hasFeature"/>
        <rdfs:domain rdf:resource="http://purl.org/olia/bambara.owl#Glose"/>
