    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/olia/bambara.owl#Adjectif -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Adjectif">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Adverbe -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Adverbe">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
        <rdfs:comment xml:lang="en">Note that Auxiliaire is not to be confused with auxiliary (verb). This category refers to closed-class expressions with primarily grammatical function. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#AutreAbreviation -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#AutreAbreviation">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Glose"/>
        <rdfs:comment xml:lang="fr">Autres abréviations utilisées dans les gloses</rdfs:comment>
        <owl:versionInfo xml:lang="en">The individuals assembled here have also been tentatively assigned to other parts of speech (as interpreted by the ontology editor)</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Auxiliaire -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Auxiliaire">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
        <owl:versionInfo xml:lang="en">Created for the glosage des auxiliaires, contains
conj 
cop 
dtm 
pers 
pm
pron 
prt</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Conjonction -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Conjonction">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Auxiliaire"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Copule -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Copule">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Auxiliaire"/>
        <rdfs:comment xml:lang="en">Note that the Bambara copula is not a verbal element. Even though it syntactically resembles a verb, it is morphologically different and non-verbal in origin. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Derivation -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Derivation">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/system.owl#Feature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/olia/bambara.owl#derivation"/>
                <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Determinatif -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Determinatif">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#PronomDeterminatif"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Flexion -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Flexion">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/system.owl#Feature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/olia/bambara.owl#flexion"/>
                <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Glose -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Glose">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/system.owl#UnitOfAnnotation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/olia/system.owl#hasTier"/>
                <owl:hasValue>POS</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Interjection -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Interjection">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#MarquePredicative -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#MarquePredicative">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Auxiliaire"/>
        <rdfs:comment xml:lang="en">Predicative markers represent the syntactic center of the sentence. They resemble, but are different from both verbs and copulas and they must not be confused with particles which serve a peripheral function in sentences, only. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#MotEtranger -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#MotEtranger">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#AutreAbreviation"/>
        <owl:versionInfo xml:lang="en">created as a cover term for ETRG...</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Nom -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Nom">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#NomPropre -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#NomPropre">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#AutreAbreviation"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Nom"/>
        <owl:versionInfo xml:lang="en">created as a cover term for NOM.F, NOM.M, etc.</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Numératif -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Numératif">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
        <rdfs:comment xml:lang="en">Numératif refers to cardinal numbers, ordinal numbers are adjectives, non-numeric quantifiers are determiners. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Onomatopee -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Onomatopee">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
        <rdfs:comment xml:lang="en">Onomatopée refers to the onomatopoetic mimicking of natural sounds, e.g., as made by an animal, when these are not lexicalized as another constituent, e.g., a noun. In this sense, Onomatopée is related to interjection. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#POS -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#POS">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Glose"/>
        <rdfs:comment>Note that not all POS categories found in the corpus are listed here, only those found under http://cormand.huma-num.fr/marques.html.
&apos;Unknown&apos; pos tags are: c, adv/n (= n/adv?), n.prop, conj/prep (= prep/conj?), mrph, _ (= unclear?), prn/dtm, n/v, v/n.  Additional rare tags not covered by the documentation include  conv.n, n.prop/dtm/prn/n/ptcp/adj (2x), n/intj (2x), vt (2x)</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">Marques des parties de discours dans le Corpus bambara de référence</rdfs:comment>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://cormand.huma-num.fr/marques.html</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Participe -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Participe">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
        <rdfs:comment xml:lang="en">As in Europen languages, Bambara participles are multi-functional, they have morphological characteristics of verbs, but often syntactic characteristics of Adjectives. (V. Vydrine)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Particule -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Particule">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Auxiliaire"/>
        <rdfs:comment xml:lang="en">Bambara particles convey modal/episthemic senses. In the BRC tagset, particle only refers to elements at the syntactic periphery of sentences. In this regard, they differ fundamentally from perdicative markers (V. Vydrine)</rdfs:comment>
        <owl:versionInfo xml:lang="en">TODO: Valentin mentioned a possible relation with determiners. To be confirmed.</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#Ponctuation -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Ponctuation">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#AutreAbreviation"/>
        <rdfs:comment xml:lang="en">added for punctuation signs (CC), not part of the original glosses, but treated differently</rdfs:comment>
    </owl:Class>
    


