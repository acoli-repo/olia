    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#Postposition">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#POS"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#PronomDeterminatif -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#PronomDeterminatif">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/bambara.owl#Auxiliaire"/>
        <owl:versionInfo xml:lang="en">created for tags (glosses) classified as &quot;dtm, pron&quot;, as no holistic term is available, we also followed EAGLES to include PronounPersonnel</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/bambara.owl#PronomNonPersonnel -->

    <owl:Class rdf:about="http://purl.org/olia/bambara.owl#PronomNonPersonnel">
