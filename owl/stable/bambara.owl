    </owl:ObjectProperty>
    


    <!-- http://purl.org/olia/bambara.owl#flexion -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/bambara.owl#flexion">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/olia/system.owl#hasFeature"/>
        <rdfs:domain rdf:resource="http://purl.org/olia/bambara.owl#Glose"/>
        <rdfs:range rdf:resource="http://purl.org/olia/bambara.owl#Flexion"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/olia/system.owl#hasFeature -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/system.owl#hasFeature"/>
    


