    <!-- http://purl.org/olia/connexor.owl#ImperativeVerb -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#ImperativeVerb">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Verb"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#IndicativeVerb -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#IndicativeVerb">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Verb"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Infinitive -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Infinitive">
