    <!-- http://purl.org/olia/connexor.owl#Abbreviation -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Abbreviation">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With abbreviations, the obligatory tags include &#39;ABBR&#39; and case. In Conexor FDG, the obligatory tags for abbreviations include &#39;ABBR&#39;, case and number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Adjective -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Adjective">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With adjectives, the obligatory tags include &#39;A&#39; and degree of comparison.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Adverb -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Adverb">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With adverbs, the obligatory tags include &#39;ADV&#39;. The anglebracket tag again occurs in front of the &#39;ADV&#39; tag, when relevant.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#AdverbSubfeatures -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#AdverbSubfeatures">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Feature"/>
        <rdfs:comment rdf:datatype="&xsd;string">features specific to Adverb</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#AdverbialFunction -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#AdverbialFunction">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#SyntacticRelation"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Aspect -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Aspect">
