                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Noun"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Number"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Numeral"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Pronoun"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
