    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#AttributivePronoun -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#AttributivePronoun">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Pronoun"/>
        <rdfs:comment rdf:datatype="&xsd;string">This is actually a combination of two annotations: &quot;PRON adj&quot;</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Axiom_1 -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Axiom_1"/>
    


    <!-- http://purl.org/olia/connexor.owl#Case -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Case">
