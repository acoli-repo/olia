    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#CoordinatingConjunction -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#CoordinatingConjunction">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#DefinitenessFeature -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#DefinitenessFeature">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Feature"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Determiner -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Determiner">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With determiners, the only obligatory tag is &#39;DET&#39;. In Conexor FDG, the obligatory tags for determiners include &#39;DET&#39; and number. In the ordering of the morphological tags, the number tag is the last one and the case tag second to last, when relevant.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#DeterminerSubfeatures -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#DeterminerSubfeatures">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Feature"/>
        <rdfs:comment rdf:datatype="&xsd;string">Features specific to determiners</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Feature -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Feature">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#MorphologicalTag"/>
        <rdfs:subClassOf rdf:resource="&olia_system;Feature"/>
        <rdfs:comment rdf:datatype="&xsd;string">Subfeatures of PartOfSpeech tags.</rdfs:comment>
    </owl:Class>
    


