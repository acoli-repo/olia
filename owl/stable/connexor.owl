    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Interjection -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Interjection">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Modification -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Modification">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#SyntacticRelation"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#MorphologicalTag -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#MorphologicalTag">
        <rdfs:subClassOf rdf:resource="&olia_system;LinguisticAnnotation"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&olia_system;hasTag"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&olia_system;hasTagContaining"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&olia_system;hasTier"/>
                <owl:hasValue rdf:datatype="&xsd;string">morph</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#NegativeParticle -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#NegativeParticle">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Noun -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Noun">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With nouns, the obligatory tags include &#39;N&#39; and case. In Conexor FDG, the obligatory tags for nouns include &#39;N&#39;, case, and number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Number -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Number">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Feature"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#Numeral -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#Numeral">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
        <rdfs:comment rdf:datatype="&xsd;string">With numerals, the obligatory tags include &#39;NUM&#39; and either &#39;CARD&#39; or &#39;ORD&#39;.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#NumeralFeatures -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#NumeralFeatures">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#Feature"/>
        <rdfs:comment rdf:datatype="&xsd;string">Subfeatures specific to Numeral</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/connexor.owl#PartOfSpeech -->

    <owl:Class rdf:about="http://purl.org/olia/connexor.owl#PartOfSpeech">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/connexor.owl#MorphologicalTag"/>
