    <!-- http://purl.org/olia/system.owl#hasTag -->

    <owl:DatatypeProperty rdf:about="&olia_system;hasTag">
        <rdfs:comment rdf:datatype="&xsd;string">Connexor produces space-separated lists of tags, every tag element assigned by this property represents one token in such a list. With different tag-tokenization, hasTag is to be renamed to &quot;containsTag&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#MorphologicalTag"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#PartOfSpeech"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/olia/system.owl#hasTagContaining -->

    <owl:DatatypeProperty rdf:about="&olia_system;hasTagContaining"/>
    


    <!-- http://purl.org/olia/system.owl#hasTagEndingWith -->

    <owl:DatatypeProperty rdf:about="&olia_system;hasTagEndingWith"/>
    


    <!-- http://purl.org/olia/system.owl#hasTagStartingWith -->

    <owl:DatatypeProperty rdf:about="&olia_system;hasTagStartingWith"/>
    


    <!-- http://purl.org/olia/system.owl#hasTier -->

    <owl:DatatypeProperty rdf:about="&olia_system;hasTier"/>
    


