    <!-- http://purl.org/olia/connexor.owl#hasAspect -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/connexor.owl#hasAspect">
        <rdfs:subPropertyOf rdf:resource="&olia_system;hasFeature"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Aspect"/>
                    <rdf:Description rdf:about="&olia_system;Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Aspect"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Verb"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/olia/connexor.owl#hasCase -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/connexor.owl#hasCase">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/olia/connexor.owl#Case"/>
        <rdfs:subPropertyOf rdf:resource="&olia_system;hasFeature"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Abbreviation"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Case"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Determiner"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Noun"/>
                    <rdf:Description rdf:about="http://purl.org/olia/connexor.owl#Pronoun"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/olia/connexor.owl#hasDegreeOfComparison -->

    <owl:ObjectProperty rdf:about="http://purl.org/olia/connexor.owl#hasDegreeOfComparison">
        <rdfs:range rdf:resource="http://purl.org/olia/connexor.owl#Comparison"/>
        <rdfs:subPropertyOf rdf:resource="&olia_system;hasFeature"/>
        <rdfs:domain>
            <owl:Class>
