    <!-- http://purl.org/olia/mte/multext-east.owl#Abbreviation -->

    <owl:Class rdf:about="http://purl.org/olia/mte/multext-east.owl#Abbreviation">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/mte/multext-east.owl#MorphosyntacticCategory"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
