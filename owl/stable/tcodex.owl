        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/olia/system.owl#hasTier"/>
                <owl:hasValue rdf:datatype="&xsd;string">givenness</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">The following tags are given to discourse refernts only. For more details on entities that have to be annotated at this level, see the SFB-Guildelines.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/tcodex.owl#GrammaticalFunction -->

    <owl:Class rdf:about="http://purl.org/olia/tcodex.owl#GrammaticalFunction">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/tcodex.owl#Morpho-Syntactic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/olia/system.owl#hasTier"/>
                <owl:hasValue rdf:datatype="&xsd;string">gf</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/olia/tcodex.owl#HangingTopic -->

    <owl:Class rdf:about="http://purl.org/olia/tcodex.owl#HangingTopic">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/tcodex.owl#InitionalPosition"/>
        <owl:versionInfo>Added in T-CODEX 2.1</owl:versionInfo>
        <rdfs:comment>hanging topic: In this construction, a dislocated element and its
resuming pronoun do not necessarily agree in
case, number and gender. (Petrova and Odebrecht 2011)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/tcodex.owl#ImperativeMainClause -->

    <owl:Class rdf:about="http://purl.org/olia/tcodex.owl#ImperativeMainClause">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/tcodex.owl#MainClause"/>
        <rdfs:comment rdf:datatype="&xsd;string">This tag is given to main sentences which use the imperative mood to utter requests.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/tcodex.owl#Indefinite -->

    <owl:Class rdf:about="http://purl.org/olia/tcodex.owl#Indefinite">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/tcodex.owl#Definiteness"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/tcodex.owl#IndefinitePronoun -->

    <owl:Class rdf:about="http://purl.org/olia/tcodex.owl#IndefinitePronoun">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/tcodex.owl#Pronoun"/>
        <rdfs:comment rdf:datatype="&xsd;string">This is a cover term for indefinites and quantifiers in OHG including items like &quot;many&quot;, &quot;all&quot;, &quot;some&quot; etc.</rdfs:comment>
    </owl:Class>
    


