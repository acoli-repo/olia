        <rdfs:comment>SUSANNE tags beginning with V and containing N (VDN, VHN and VVN) represent past participles.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#Person -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#Person">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/system.owl#Feature"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#PostmodifyingAdverb -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#PostmodifyingAdverb">
