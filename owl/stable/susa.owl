    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#Number -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#Number">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/system.owl#Feature"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#Numeral -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#Numeral">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/susa.owl#Tag"/>
        <rdfs:comment>The Numeral class subsumes cardinal numbers, ordinal numbers and fraction numbers. The SUSANNE tags begin with M.. .</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#OrdinalNumber -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#OrdinalNumber">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/susa.owl#Numeral"/>
