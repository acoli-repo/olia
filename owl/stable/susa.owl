    <!-- http://purl.org/olia/susa.owl#APPGm -->

    <owl:NamedIndividual rdf:about="http://purl.org/olia/susa.owl#APPGm">
        <rdf:type rdf:resource="http://purl.org/olia/susa.owl#AttributivePossessivePronoun"/>
        <hasCase rdf:resource="http://purl.org/olia/susa.owl#genitive"/>
