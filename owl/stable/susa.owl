    <!-- http://purl.org/olia/susa.owl#LexicalVerbPastForm -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#LexicalVerbPastForm">
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/susa.owl#LexicalVerb"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/olia/susa.owl#Past"/>
        <rdfs:comment>SUSANNE tags beginning with VV... are used for non-modal verbs or non-auxiliary verbs. 
LexicalVerbPastForm is a class for past tense verbforms. 

(Sampson 1995, p. 118f)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/susa.owl#LexicalVerbPastParticiple -->

    <owl:Class rdf:about="http://purl.org/olia/susa.owl#LexicalVerbPastParticiple">
