    <owl:Class rdf:about="&turkish;DeverbalAdverb">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Adv derived from verbs: +AfterDoingSo, +SinceDoingSo, +As (he
does it), +When, +ByDoingSo, +While, +AsIf,
+WithoutHavingDoneSo.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DeverbalVerb -->

    <owl:Class rdf:about="&turkish;DeverbalVerb">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>Verbs have two sets of markers which are treated as derivations: Voice and Compounding/Modality</rdfs:comment>
    </owl:Class>
    


