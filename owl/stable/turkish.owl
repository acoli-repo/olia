    <!-- http://purl.org/olia/turkish.owl#DeadjectivalAdverb -->

    <owl:Class rdf:about="&turkish;DeadjectivalAdverb">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Adv derived from Adjectives: +Ly (equivalent to the English +ly derivation.)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DeadjectivalNoun -->

    <owl:Class rdf:about="&turkish;DeadjectivalNoun">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Noun derived from adjectives: +Ness (as in red vs. redness)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DemonsPron -->

    <owl:Class rdf:about="&turkish;DemonsPron">
        <rdfs:subClassOf rdf:resource="&turkish;Pron"/>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DenominalAdjective -->

    <owl:Class rdf:about="&turkish;DenominalAdjective">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Adj derived from nouns: +With, +Without, +SuitableFor,
+InBetween, +Rel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DenominalAdverb -->

    <owl:Class rdf:about="&turkish;DenominalAdverb">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Adv derived from temporal nouns: +Since</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DenominalDeadjectivalVerb -->

    <owl:Class rdf:about="&turkish;DenominalDeadjectivalVerb">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Verb derived from nouns or adjectives: +Become (to become like the noun
or adjective in the stem) +Acquire (to acquire the noun in the stem)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DenominalNoun -->

    <owl:Class rdf:about="&turkish;DenominalNoun">
        <rdfs:subClassOf rdf:resource="&turkish;DerivationType"/>
        <rdfs:comment>After +Noun derived from nouns: +Agt (someone involved in some way with the
stem noun), +Dim (Diminutive),</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DerivationType -->

    <owl:Class rdf:about="&turkish;DerivationType">
        <rdfs:subClassOf rdf:resource="&system;Feature"/>
        <owl:versionInfo>These derivations occur subsequently. With the current modelling with subClassOf relationships, this subsequent derivation cannot be expressed.

The ontology thus covers only the annotations attached to a morpheme, it thus presupposes a morpheme-level segmentation.</owl:versionInfo>
    </owl:Class>
    


    <!-- http://purl.org/olia/turkish.owl#DesireWish -->

    <owl:Class rdf:about="&turkish;DesireWish">
        <rdfs:subClassOf rdf:resource="&turkish;Tense-Aspect-Mood"/>
        <rdfs:comment>desire/wish</rdfs:comment>
    </owl:Class>
    


